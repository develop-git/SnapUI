{
  "name": "SnapUI",
  "version": "1.0.0",
  "license": "MIT",
  "summary": "对 SnapKit 进行链式编程扩展，同时避免了视图添加约束冲突、视图未添加到父视图等造成崩溃的问题",
  "homepage": "https://gitee.com/jianlisoft/snap-ui",
  "authors": {
    "jianli": "develop-work@outlook.com"
  },
  "source": {
    "git": "git@gitee.com:jianlisoft/snap-ui.git",
    "tag": "1.0.0"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "11.0",
    "osx": "11.12",
    "tvos": "11.0"
  },
  "swift_versions": [
    "5.5"
  ],
  "source_files": "Sources/**/*.swift",
  "dependencies": {
    "SnapKit": [
      "~> 5.6.0"
    ]
  },
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(SDKROOT)/usr/lib/swift"
  },
  "swift_version": "5.5"
}
